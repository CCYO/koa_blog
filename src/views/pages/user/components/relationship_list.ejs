<% for( _type in relationShip){ %>
<% let _list = relationShip[_type] %>
<div class="card">
  <div class="card-header">
    <h5><%= _type === "fansList" ? "粉絲" : "偶像" %>列表</h5>
  </div>
  <div class="card-body">
    <div class="betterscroll-outer" id="<%= _type %>">
      <% if(_list.length) {%>
      <ul class="list-group list-group-horizontal">
        <% _list.forEach( _user => { %>
        <%- ejs_render.user.relationshipItem({ user: _user }) %>
        <% }) %>
      </ul>
      <% } else { %>
      <p>很遺憾，列表裡沒有人</p>
      <% } %>
    </div>
  </div>
</div>
<% } %>