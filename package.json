{
  "name": "koa-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "nocache": "npm run _dev_build && cross-env NODE_ENV=nocache nodemon",
    "dev": "npm run _dev_build && cross-env NODE_ENV=development nodemon",
    "build": "rm -rf ./server/views && npm run _common_build && cross-env NODE_ENV=production webpack --config ./build/webpack.prod.config.js --env analyzer=true",
    "start": "pm2 start ./server/_config/ecosystem.config.js",
    "status": "pm2 status",
    "log": "pm2 logs koa_blog",
    "prod": "npm run build -- --env analyzer=false && npm start",
    "re": "pm2 restart koa_blog",
    "update": "npm run build -- --env analyzer=false && npm run re",
    "stop": "pm2 stop koa_blog",
    "delete": "pm2 delete koa_blog",
    "clear": "pm2 delete koa_blog || true && pm2 flush",
    "sync_db": "node ./server/db/mysql/sync",
    "_common_build": "cd ./common && webpack --config ./webpack.config.js",
    "_dev_build": "rm -rf ./server/dev_views && npm run _common_build -- --env=dev"
  },
  "dependencies": {
